<?xml version="1.0" standalone="no"?>
<!DOCTYPE product_file SYSTEM "../productfiles.dtd">

<!--
    Notes:
        See ../productfiles.dtd in this directory for full notes.  All DTD information
        is contained there.
-->
<product_file>
<product method='externalalg' name='winds' testonly='no' finalonly='no'>
    <externalalg_args>
        <source name='winds' granule_composites='no'>
            <var>speed</var>
            <var>direction</var>
            <var>lats</var>
            <var>lons</var>
        </source>
        <day_night>both</day_night>
        <cmap>tropix_no_white</cmap>
        <colorbar>
            <cmap>tropix_no_white</cmap>
            <ticks>0 5 10 15 20 25 30 35 40 45 50 55 60</ticks>
            <title>knots</title>
        </colorbar>
    </externalalg_args>
</product>
</product_file>

