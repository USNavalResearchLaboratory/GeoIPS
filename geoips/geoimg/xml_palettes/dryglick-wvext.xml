<?xml version="1.0" standalone="no"?>
<!DOCTYPE colorbar_list SYSTEM "colorbars.dtd">

<colorbar_list xmlns:py="http://codespeak.net/lxml/objectify/pytype">
    <colorbar name="dryglick-wvext">
        <min_val>-90</min_val>
        <max_val>10</max_val>
        <transition>
            <start_color>(.3,.1,.5)</start_color>
            <end_color>(.8,.8,.9)</end_color>
            <start_val>-90</start_val>
            <end_val>-80</end_val>
        </transition>
        <transition>
            <start_color>(0,.9,0)</start_color>
            <end_color>(0,.3,0)</end_color>
            <start_val>-80</start_val>
            <end_val>-70</end_val>
        </transition>
        <transition>
            <start_color>(0,0,1)</start_color>
            <end_color>(0,0,.1)</end_color>
            <start_val>-70</start_val>
            <end_val>-60</end_val>
        </transition>
        <transition>
            <start_color>(0,.9,.9)</start_color>
            <end_color>(.1,.22,.35)</end_color>
            <start_val>-60</start_val>
            <end_val>-50</end_val>
        </transition>
        <transition>
            <start_color>(1,1,1)</start_color>
            <end_color>(.1,.1,.1)</end_color>
            <start_val>-50</start_val>
            <end_val>-40</end_val>
        </transition>
        <transition>
            <start_color>(.2,.2,0)</start_color>
            <end_color>(.9,.9,0)</end_color>
            <start_val>-40</start_val>
            <end_val>-30</end_val>
        </transition>
        <transition>
            <start_color>(1,.5,0)</start_color>
            <end_color>(.3,0,0)</end_color>
            <start_val>-30</start_val>
            <end_val>-20</end_val>
        </transition>
        <transition>
            <start_color>(.2,0,0)</start_color>
            <end_color>(1,0,0)</end_color>
            <start_val>-20</start_val>
            <end_val>-10</end_val>
        </transition>
        <transition>
            <start_color>(1,.3,.3)</start_color>
            <end_color>(.2,.08,.08)</end_color>
            <start_val>-10</start_val>
            <end_val>0</end_val>
        </transition>
        <transition>
            <start_color>(.3,0,.3)</start_color>
            <end_color>(.9,.5,.9)</end_color>
            <start_val>0</start_val>
            <end_val>10</end_val>
        </transition>
    </colorbar> 
</colorbar_list>
